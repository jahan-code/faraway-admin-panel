{
  "name": "liquid-dms-admin-panel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_TELEMETRY_DISABLED=1 next dev -p 3010",
    "dev:full": "concurrently \"npm run dev\" \"wait-on http://localhost:3010 && set PATH=%USERPROFILE%\\.cargo\\bin;%PATH% && npx tauri dev\"",
    "dev:tauri": "concurrently \"npm run dev\" \"wait-on http://localhost:3010 && set PATH=%USERPROFILE%\\.cargo\\bin;%PATH% && npx tauri dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:tauri": "npm run build && npx tauri build",
    "voice:list": "powershell -NoLogo -NoProfile -ExecutionPolicy Bypass -File \"tools/voice/list-voices.ps1\"",
    "voice:say": "powershell -NoLogo -NoProfile -ExecutionPolicy Bypass -File \"tools/voice/speak-text.ps1\"",
    "voice:clipboard": "powershell -NoLogo -NoProfile -ExecutionPolicy Bypass -File \"tools/voice/speak-clipboard.ps1\"",
    "voice:dictate": "powershell -NoLogo -NoProfile -ExecutionPolicy Bypass -File \"tools/voice/dictate-to-caret.ps1\" -Culture en-US",
    "voice:test": "powershell -NoLogo -NoProfile -ExecutionPolicy Bypass -File \"tools/voice/speak-text.ps1\" -VoiceName \"Microsoft David Desktop\" -Text \"Hello, I am your Jarvis assistant. Voice is online.\"",
    "voice:auto": "powershell -NoLogo -NoProfile -ExecutionPolicy Bypass -File \"tools/voice/auto-speak-clipboard.ps1\" -IntervalMs 500 -VoiceName \"Microsoft David Desktop\" -Rate 0 -Volume 100",
    "voice:auto:zira": "powershell -NoLogo -NoProfile -ExecutionPolicy Bypass -File \"tools/voice/auto-speak-clipboard.ps1\" -IntervalMs 500 -VoiceName \"Microsoft Zira Desktop\" -Rate 0 -Volume 100"
  },
